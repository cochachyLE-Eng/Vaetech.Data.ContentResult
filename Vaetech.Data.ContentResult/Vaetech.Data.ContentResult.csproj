<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net461;net48;net5.0;net6.0</TargetFrameworks>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Company>LIIKSOFT</Company>
    <Authors>Luis Eduardo Cochachi Chamorro</Authors>
    <Version>1.8.11.1</Version>    
    <PackageIcon>package.png</PackageIcon>
    <PackageIconUrl />
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	<PackageLicenseExpression>MIT OR Apache-2.0</PackageLicenseExpression>
	<RepositoryUrl>https://github.com/cochachyLE-Eng/Vaetech.Data.ContentResult.git</RepositoryUrl>
	<PackageProjectUrl>https://github.com/cochachyLE-Eng/Vaetech.Data.ContentResult</PackageProjectUrl>
	<PackageId>Vaetech.Data.ContentResult</PackageId>
	<PackageTags>ContentResult, EntityFramework, Dapper, ActionResult, TupleResult, Trycatch</PackageTags>
	<Description>Dynamic Response Container for NetFramework, NetStandard, NetCore and Net</Description>
	<SignAssembly>True</SignAssembly>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\icons\package.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>    
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.1'">
	  <PackageReference Include="System.Text.Json" Version="6.0.7" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net461'">
	  <PackageReference Include="System.Text.Json" Version="6.0.7" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net48'">
	  <PackageReference Include="System.Text.Json" Version="6.0.7" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net5.0'">
	  <PackageReference Include="System.Text.Json" Version="6.0.7" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
	  <PackageReference Include="System.Text.Json" Version="6.0.7" />
  </ItemGroup>

</Project>
